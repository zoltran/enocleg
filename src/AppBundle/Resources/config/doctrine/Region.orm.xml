<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Region" table="region">
    <id name="regionid" type="integer" column="regionId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="regionname" type="string" column="regionName" length="55" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="regionkat" type="integer" column="regionKat" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>

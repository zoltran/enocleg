<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Users" table="users">
    <id name="userid" type="integer" column="userId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="userName" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="userpass" type="string" column="userPass" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="useremail" type="string" column="userEmail" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="userfirma" type="string" column="userFirma" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userfrom" type="string" column="userFrom" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="userulica" type="string" column="userUlica" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userpoczta" type="string" column="userPoczta" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usernip" type="string" column="userNip" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userregon" type="string" column="userRegon" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usertel" type="string" column="userTel" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usertel2" type="string" column="userTel2" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="useremail2" type="string" column="userEmail2" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userimie" type="string" column="userImie" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usernazwisko" type="string" column="userNazwisko" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userpesel" type="string" column="userPesel" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userdate" type="datetime" column="userDate" nullable="false">
      <options>
        <option name="default">0000-00-00 00:00:00</option>
      </options>
    </field>
    <field name="userhash" type="string" column="userHash" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="userconfirm" type="integer" column="userConfirm" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
